{
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",
    "strict": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "noImplicitAny": false,
    "lib": [
      "esnext",
      "dom"
    ],
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
    },
    "outDir": "dist"
  },
  "include": [
    "**/*"
  ],
  "exclude": [],
}
